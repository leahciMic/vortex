foo := {
  a: 10,
  b: 2,
};

x := 1; // #info
y := 1; // #info #warning #info
foo.a = x := 2; // #error #error #error
foo.a = (x = (y := 2)); // #error #error #error (creation just more deeply nested)
foo.a = (x.y := 2); // #error also not ok (removed exception for adding props)
foo.c := 3;
foo.d := {};
y := 3; // #error

func() { // TODO: top level anonymous function can't be called
  a := 1; // #warning
  return b := 1; // #error #warning
};

a++; // #error

foo.d.e := 42; // #error
